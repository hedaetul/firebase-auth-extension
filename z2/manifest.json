{
    "manifest_version": 3,
    "name": "Firebase Auth Extension",
    "version": "1.0",
    "description": "A simple Chrome extension for Firebase authentication",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    }
  }
  
  